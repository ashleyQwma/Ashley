local MainTab = Window:MakeTab({
    Name = "الاغاني | Music",
    Icon = "rbxassetid://16905163783",
    PremiumOnly = false
})
MainTab:AddLabel("------------------------------------------ سگيت بورد ------------------------------------------")
MainTab:AddTextbox({
  Name = "ادخل كود الاغنية داخل المربع - سكيت بورد ",
  Default = "",
  TextDisappear = false,
  Callback = function(Value)
    local args1 = {
            [1] = "SkateBoard"
        }
        game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1NoMoto1rVehicle1s"):FireServer(unpack(args1))
 
        -- Replacing the static value with the input from the textbox
        local args2 = {
            [1] = "PickingScooterMusicText",
            [2] = Value -- Here, we use the textbox value to replace 35935204
        }
        game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1NoMoto1rVehicle1s"):FireServer(unpack(args2))
    end
})

MainTab:AddLabel("------------------------------------------ حط كود الاغنية السـيارة ------------------------------------------")
 

MainTab:AddTextbox({
  Name = "ادخل كود الاغنية داخل المربع - سيارة ",
  Default = "",
  TextDisappear = false,
  Callback = function(Value)
    local args1 = {
            [1] = "SkateBoard"
        }
        game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1NoMoto1rVehicle1s"):FireServer(unpack(args1))
 
        -- Replacing the static value with the input from the textbox
        local args2 = {
            [1] = "PickingScooterMusicText",
            [2] = Value -- Here, we use the textbox value to replace 35935204
        }
        game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1NoMoto1rVehicle1s"):FireServer(unpack(args2))
    end
})
MainTab:AddLabel("------------------------------------------ حط كـود الاغنية البيـت ------------------------------------------")
 
MainTab:AddTextbox({
  Name = "ادخل كود الاغنية  داخل بوكس ",
  Default = "",
  TextDisappear = false,
  Callback = function(input)
local args = {
            [1] = "PickHouseMusicText",
            [2] = input -- Use the user's input here
        }
        game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1Player1sHous1e"):FireServer(unpack(args))
    end
})
MainTab:AddButton({
  Name = "صوت جوال اندرويد عالي",
  Callback = function()
    	local args = {
    [1] = "SkateBoard"
}
 
game:GetService("ReplicatedStorage").RE:FindFirstChild("1NoMoto1rVehicle1s"):FireServer(unpack(args))
local args = {
    [1] = "PickingScooterMusicText",
    [2] = "7288899492"
}
 
