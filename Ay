local Tab = Window:MakeTab({"FUN", ""})
Tab:AddSection({"FLING V1"})

local players = {"..."}

for _, player in pairs(game.Players:GetPlayers()) do
    table.insert(players, player.Name)
end
Tab:AddDropdown({
  Name = "Choose a player",
  Description = "",
  Options = players,
  Default = "1",
  Flag = "dropdown teste",
  Callback = function(Value)
    getgenv().selectedPlayer = Value
  end
})
Tab:AddButton({"fling player", function()
                local selectedPlayer = getgenv().selectedPlayer  -- تأكد من أن اسم اللاعب الذي تم اختياره موجود هنا
        local player = game.Players.LocalPlayer  -- هذه هي شخصيتك

        if selectedPlayer then
            local targetPlayer = game.Players:FindFirstChild(selectedPlayer)  -- إيجاد اللاعب الذي تم اختياره

            if targetPlayer then
                -- نقل الأداة "Couch" من الـ Backpack إلى الشخصية في كل مرة يتم الضغط فيها على الزر
                local couchInBackpack = nil
                -- التحقق إذا كانت الأداة "Couch" موجودة في الـ Backpack
                for _, item in pairs(player.Backpack:GetChildren()) do
                    if item.Name == "Couch" then
                        couchInBackpack = item
                        break
                    end
                end

                -- إذا كانت الأداة "Couch" موجودة في الـ Backpack، نقوم بنقلها إلى الـ Character
                if couchInBackpack then
                    couchInBackpack.Parent = player.Character  -- نقل الأداة إلى الشخصية
                    print("تم نقل الأداة 'Couch' إلى الشخصية.")
                else
                    -- إذا كانت الأداة "Couch" غير موجودة في الـ Backpack، نقوم بتفعيلها
                    local args = {[1] = "PickingTools", [2] = "Couch"}
                    game:GetService("ReplicatedStorage").RE:FindFirstChild("1Too1l"):InvokeServer(unpack(args))
                    print("تم تفعيل الأداة: Couch")
                end

                -- الانتقال إلى اللاعب المحدد (الذي تم اختياره) ولكن مع تعديل الموقع ليكون تحت اللاعب قليلاً
                if targetPlayer.Character and targetPlayer.Character.PrimaryPart then
                    -- تعديل موقع CFrame بحيث تكون الشخصية الخاصة بك تحت اللاعب قليلاً
                    local targetPosition = targetPlayer.Character.PrimaryPart.Position
                    local newPosition = targetPosition - Vector3.new(0, 5, 0)  -- تقليل الإحداثيات في المحور Y ليكون تحت اللاعب قليلاً
                    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(newPosition))
                    print("تم الانتقال إلى: " .. selectedPlayer)
                end

                -- دوران الشخصية بشكل متقلب وكامل حول جميع المحاور
                local startTime = tick()  -- وقت البداية
                local oscillationTime = 0  -- وقت التذبذب
                while tick() - startTime < .6 do  -- التكرار لمدة 2 ثانية
                    if player.Character and player.Character.PrimaryPart then
                        -- تدوير الشخصية بشكل سريع حول المحاور الثلاثة (X، Y، Z) ليكون مثل حركة "Fling"
                        player.Character:SetPrimaryPartCFrame(player.Character.PrimaryPart.CFrame * CFrame.Angles(
                            math.rad(math.random(500, 1000)),  -- تدوير عشوائي حول محور X
                            math.rad(math.random(500, 1000)),  -- تدوير عشوائي حول محور Y
                            math.rad(math.random(500, 1000))   -- تدوير عشوائي حول محور Z
                        ))

                        -- تحريك الشخصية للأعلى والأسفل (التذبذب)
                        local yOffset = math.sin(oscillationTime) * 5  -- استخدام sin للتذبذب
                        player.Character:SetPrimaryPartCFrame(player.Character.PrimaryPart.CFrame * CFrame.new(0, yOffset, 0))
                        oscillationTime = oscillationTime + 0.1  -- زيادة الزمن لجعل التذبذب مستمر
                    end
                    wait(0.001)  -- الانتظار لمدة 0.01 ثانية لضمان سلاسة الحركة
                end

                -- الانتظار لمدة 0.5 ثانية قبل التحرك إلى مكان بعيد
                wait(0.5)

                -- تحريك الشخصية إلى مكان بعيد جدًا خارج الماب
                local outsideMapPosition = Vector3.new(10000, 0, 10000)  -- تغيير القيم لهذه النقطة بعيدة
                game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(outsideMapPosition))
                print("تم نقل الشخصية إلى مكان بعيد.")

                -- الانتظار لمدة قصيرة قبل العودة
                wait(0.5)

                -- بعد العودة من المكان البعيد، نبحث عن الأداة "Couch" في الشخصية
                local couchInCharacter = nil
                if player.Character then
                    for _, item in pairs(player.Character:GetChildren()) do
                        if item.Name == "Couch" then
                            couchInCharacter = item
                            break
                        end
                    end
                end

                -- إذا كانت الأداة "Couch" موجودة في الشخصية، نقوم بإرجاعها إلى الـ Backpack
                if couchInCharacter then
                    couchInCharacter.Parent = player.Backpack  -- إرجاع الأداة إلى الـ Backpack
                    print("تم إعادة الأداة 'Couch' إلى الـ Backpack.")
                end

                -- العودة إلى المكان الأصلي
                if targetPlayer.Character and targetPlayer.Character.PrimaryPart then
                    local returnPosition = targetPlayer.Character.PrimaryPart.Position - Vector3.new(0, 5, 0)  -- العودة إلى مكان تحت اللاعب
                    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(returnPosition))
                    print("تم العودة إلى المكان الأصلي.")
                end
            end
        end
            end})
Tab:AddSection({"FLING V2 "})
 local selectedKillAdvancedPlayer = nil
local couchEquipped = false

local function killAdvancedPlayer()
if selectedKillAdvancedPlayer then
local player = game.Players:FindFirstChild(selectedKillAdvancedPlayer)
if player then
-- Equipa o item 'Couch' no inventário se ainda não estiver equipado
local backpack = game.Players.LocalPlayer.Backpack
if backpack and not couchEquipped then
local couch = backpack:FindFirstChild("Couch")
if couch then
game.Players.LocalPlayer.Character.Humanoid:EquipTool(couch)
couchEquipped = true
else
print("O item 'Couch' não foi encontrado no seu inventário.")
end
end

-- Looping de teleportes no jogador selecionado da lista
while true do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
wait(0.0) -- Intervalo entre cada teleporte, ajuste conforme necessário

-- Verifica se o jogador sentou no 'Couch' e realiza o teleporte para o céu
if player.Character:FindFirstChild("Humanoid") and player.Character.Humanoid.SeatPart then
player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 0, 0) -- Teleporta para cima
wait(0.0) -- Espera um pouco antes de teleportar de volta para evitar bugs
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, 0, 0) -- Teleporta para cima novamente
wait(0.0) -- Espera um pouco antes de teleportar de volta para evitar bugs
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(649.78, -439.87, 126.35) -- Teleporta de volta para a posição original
break -- Sai do loop após teleportar de volta
end
end

-- Remove o item 'Couch' da mão do jogador após o teleporte para o céu
if couchEquipped then
local backpack = game.Players.LocalPlayer.Backpack
if backpack then
local couch = backpack:FindFirstChild("Couch")
if couch then
couch.Parent = nil -- Remove o 'Couch' do inventário
couchEquipped = false
end
end
end
else
print("Jogador não encontrado.")
end
else
print("Nenhum jogador selecionado para o Bring Avançado.")
end
end

-- Lista de Players para Bring Avançado
